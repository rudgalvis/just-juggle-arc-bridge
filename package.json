{
  "name": "@jj/just-jugle-arc-bridge",
  "productName": "Just Juggle - Arc Bridge",
  "version": "0.1.0",
  "description": "App to bridge Just Juggle and Arc",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "electron-forge start",
    "package": "dotenv -e .env.development -- electron-forge package",
    "make": "dotenv -e .env.development -- electron-forge make",
    "publish": "dotenv -e .env.development -- electron-forge publish",
    "distribute": "dotenv -e .env.development -- node ./package-distribution.js",
    "lint": "eslint --ext .ts,.tsx .",
    "demo": "npm run build:native-active-window && npm run demo:server",
    "demo:server": "ts-node --project tsconfig.test.json src/services/server2.ts",
    "build:native-active-window": "cd ../node-active-window && npm run build && cp -r build ../bridge-beta/src/services/native-active-window && cd ../bridge-beta",
    "clean-run": "ts-node --project tsconfig.test.json src/services/clean.ts",
    "dev-server": "dotenv -e .env.development -- nodemon --watch src src/server/local-serve-dev.ts"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron-forge/shared-types": "^7.4.0",
    "@electron/fuses": "^1.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "electron": "31.0.0-beta.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "inquirer": "^9.2.20",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.1.6",
    "vitest": "^1.6.1"
  },
  "keywords": [],
  "author": {
    "name": "Rokas Rudgalvis",
    "email": "rokas@rudgalvis.com"
  },
  "license": "MIT",
  "dependencies": {
    "@paymoapp/active-window": "^2.1.2",
    "active-win": "^9.0.0",
    "cors": "^2.8.5",
    "electron-auto-launch": "^5.0.7",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.3",
    "get-windows": "^9.2.0",
    "nodemon": "^3.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "ws": "^8.18.3"
  }
}
